    },
    {
      "id": 3575101739082570,
      "definition": {
        "title": "Payments created (count)",
        "type": "query_value",
        "requests": [
          {
            "response_format": "scalar",
            "queries": [
              {
                "data_source": "metrics",
                "name": "query1",
                "query": "sum:payment.created{$env, $service, $bank}.as_count()",
                "aggregator": "sum"
              }
            ],
            "formulas": [
              {
                "formula": "query1",
                "number_format": {
                  "unit": {
                    "type": "canonical_unit",
                    "unit_name": "euro"
                  }
                }
              }
            ],
            "conditional_formats": [
              {
                "comparator": ">=",
                "value": 10000,
                "palette": "custom_bg",
                "custom_bg_color": "#632ca6"
              },
              {
                "comparator": "<",
                "value": 10000,
                "palette": "white_on_green"
              }
            ]
          }
        ],
        "autoscale": true,
        "precision": 0
      },
      "layout": {
        "x": 4,
        "y": 0,
        "width": 4,
        "height": 2
      }
    },
